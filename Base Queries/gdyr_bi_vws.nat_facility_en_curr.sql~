-- 2014-08-06 -> SQL Server import query definition for EDW.dbo.FacilityHierarchy

SELECT 
    FAC.FACILITY_ID
    , FAC.FACILITY_NAME
    , FAC.XTND_NAME
	, FAC.FACILITY_TYPE_ID
	, FAC.CITY_ID
	, FAC.CNTRY_ID
	, FAC.DISTR_CHAN_CD
	, FAC.DISTRICT_ID
	, FAC.DIV_CD
	, FAC.EU_USAGE_CD
	, FAC.FACILITY_CAT_ID
	, FAC.INT_CUST_ID
	, FAC.PURCH_ORG_ID
	, FAC.SALES_ORG_CD
	, FAC.TERR_ID
	, FAC.VAL_AREA_ID
	, FAC.VENDOR_CUST_ID
	, FAC.ACTIV_REQ_PLAN_IND
	, FAC.FACILITY_ACTIV_IND
	, FAC.SPLC_CD
	, FAC.SRC_LOC_CD

FROM GDYR_BI_VWS.NAT_FACILITY_EN_CURR FAC


ORDER BY
    FAC.FACILITY_ID
